- if user_signed_in?
  - if post.picked_by?(current_user)
    = link_to post_picks_path(post.id),method: :delete,remote: true do
      %span= fa_icon "get-pocket alredy_icon"
      = post.picks.size 
  - else
    = link_to post_picks_path(post.id),method: :post,remote: true do
      %span= fa_icon "get-pocket yet_icon"
      = post.picks.size
- else
  %span= fa_icon "get-pocket"
  = post.picks.size